{
    "version": 2,
    "builds": [
        {
            "src": "./app.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.8"
            }
        }
    ],
    "routes": [
        {
            "src": "/<path:any>",
            "dest": "/"
        }
    ],
    "env": {
        "GOOGLE_GENERATIVE_AI_API_KEY": "----------------------------"
    }
}
